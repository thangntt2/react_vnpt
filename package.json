{
  "name": "saga-login-flow",
  "version": "0.0.0",
  "description": "A login/register flow built with React & Redux Saga",
  "scripts": {
    "start": "node server.js",
    "build": "webpack --config webpack/prod.js --progress --colors -p",
    "test": "standard | snazzy --verbose && NODE_ENV=test ava --verbose",
    "deploy": "npm run build && surge -p build -d saga-login-flow.surge.sh"
  },
  "repository": "sotojuan/login-flow",
  "author": {
    "name": "Juan Soto",
    "email": "juan@juansoto.me",
    "url": "juansoto.me"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sotojuan/saga-login-flow/issues"
  },
  "homepage": "https://github.com/sotojuan/saga-login-flow#readme",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "ava": "^0.16.0",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "babel-runtime": "^6.9.2",
    "css-loader": "^0.23.1",
    "html-webpack-plugin": "^2.18.0",
    "localStorage": "^1.0.3",
    "postcss-focus": "^1.0.0",
    "postcss-import": "^8.1.1",
    "postcss-loader": "^0.10.0",
    "postcss-reporter": "^1.3.3",
    "postcss-simple-vars": "^3.0.0",
    "redux-ava": "^2.1.0",
    "snazzy": "^4.0.0",
    "standard": "^7.1.1",
    "style-loader": "^0.13.1",
    "surge": "^0.18.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "auth0": "^2.3.1",
    "babel-polyfill": "^6.9.1",
    "bcryptjs": "^2.3.0",
    "btoa": "^1.1.2",
    "json-loader": "^0.5.4",
    "jwt-decode": "^2.1.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "redux": "^3.5.1",
    "redux-ava": "^2.0.0",
    "redux-logger": "^2.6.1",
    "redux-saga": "^0.10.3",
    "requestify": "^0.2.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "plugins": [
      "transform-runtime",
      "transform-object-rest-spread"
    ]
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}
